<% if(successMsg && successMsg.length) {%>
    <div class="alert alert-success alert-dismissible fade show col-6 offset-3 mt-3" data-bs-dismiss="alert" role="alert">
        <%= successMsg %>
        <button class="btn-close"></button>
    </div>
<% } %>
<% if(errorMsg && errorMsg.length){ %>
    <div class="alert alert-danger alert-dismissible fade show col-6 offset-3 mt-3" data-bs-dismiss="alert" role="alert">
        <%= errorMsg %>
        <button class="btn-close"></button>
    </div>
<% } %>